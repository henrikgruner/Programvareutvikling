{
  "name": "auksjonsbua-webapp",
  "description": "A auction webpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "cross-env NODE_ENV=production && serve -s build",
    "build": "cross-env NODE_ENV=production react-scripts build",
    "heroku-postbuild": "npm run build",
    "test": "CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "lint": "npm run lint:prettier",
    "lint:prettier": "prettier '**/*.{js,css,md}' --list-different",
    "prettier": "prettier '**/*.{js,css,md}' --write",
    "cypress:open": "cypress open",
    "cypress": "cypress run",
    "start-server": "npm start",
    "test:ci": "start-server-and-test start-server http://localhost:3000 cypress"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "antd": "^3.13.6",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "date-fns": "^2.0.0-alpha.27",
    "enzyme-to-json": "^3.3.5",
    "expect-enzyme": "^1.3.0",
    "formik": "^1.5.0",
    "jest-styled-components": "^6.3.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-axios": "^2.0.3",
